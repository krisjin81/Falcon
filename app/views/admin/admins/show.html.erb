<p>
  <strong><%= Admin.human_attribute_name(:email) %>:</strong><br>
  <%= resource.email %>
</p>
<p>
  <strong><%= Admin.human_attribute_name(:admin_level) %>:</strong><br>
  <%= resource.admin_level_humanize %>
</p>
<p>
  <strong><%= AdminProfile.human_attribute_name(:username) %>:</strong><br>
  <%= resource.admin_profile.username rescue '' %>
</p>
<p>
  <strong><%= AdminProfile.human_attribute_name(:first_name) %>:</strong><br>
  <%= resource.admin_profile.first_name rescue '' %>
</p>
<p>
  <strong><%= AdminProfile.human_attribute_name(:last_name) %>:</strong><br>
  <%= resource.admin_profile.last_name rescue '' %>
</p>
<p>
<p>
  <strong><%= Admin.human_attribute_name(:last_sign_in_at) %>:</strong><br>
  <%= resource.last_sign_in_at ? l(resource.last_sign_in_at) : t('helpers.never') %>
</p>
<p>
  <strong><%= Admin.human_attribute_name(:created_at) %>:</strong><br>
  <%= resource.created_at %>
</p>
<p>
  <strong><%= Admin.human_attribute_name(:updated_at) %>:</strong><br>
  <%= resource.updated_at %>
</p>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")), collection_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")), edit_resource_path, :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")), resource_path, :method => 'delete', :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')), :class => 'btn btn-danger' %>
</div>
